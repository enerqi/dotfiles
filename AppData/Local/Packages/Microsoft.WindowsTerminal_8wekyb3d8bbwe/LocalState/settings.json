{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "scrollDownPage",
            "keys": "ctrl+shift+pgdn"
        },
        {
            "command": 
            {
                "action": "commandPalette"
            },
            "keys": "ctrl+shift+p"
        },
        {
            "command": 
            {
                "action": "newTab"
            },
            "keys": "ctrl+t"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": 
            {
                "action": "scrollDown"
            },
            "keys": "ctrl+shift+down"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": 
            {
                "action": "closeTab"
            },
            "keys": "ctrl+shift+w"
        },
        {
            "command": 
            {
                "action": "nextTab"
            },
            "keys": "ctrl+tab"
        },
        {
            "command": 
            {
                "action": "prevTab"
            },
            "keys": "ctrl+shift+tab"
        },
        {
            "command": 
            {
                "action": "scrollUp"
            },
            "keys": "ctrl+shift+up"
        },
        {
            "command": "scrollUpPage",
            "keys": "ctrl+shift+pgup"
        }
    ],
    "copyFormatting": "none",
    "copyOnSelect": true,
